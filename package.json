{
  "name": "vue-simple-chatbot",
  "version": "0.1.3",
  "browser": "dist/vue-simple-chatbot.umd.js",
  "module": "dist/vue-simple-chatbot.common.js",
  "unpkg": "dist/vue-simple-chatbot.umd.min.js",
  "scripts": {
    "dev": "vue-cli-service serve ./example/main.js",
    "lint": "vue-cli-service lint",
    "build-lib": "vue-cli-service build --target lib --name vue-simple-chatbot ./src/index.js",
    "build-example": "vue-cli-service build ./example/main.js"
  },
  "peerDependencies": {
    "vue": "3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.0",
    "@vue/cli-plugin-eslint": "4.5.0",
    "@vue/cli-service": "4.5.0",
    "@vue/compiler-sfc": "3.0.0",
    "babel-eslint": "10.1.0",
    "eslint": "6.7.2",
    "vue": "3.0.0",
    "core-js": "3.6.5",
    "eslint-plugin-vue": "7.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "description": "simple chatbot ui",
  "main": "dist/vue-simple-chatbot.common.js",
  "files": [
    "dist/*",
    "package.json"
  ],
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asmfadholi/vue-simple-chatbot.git"
  },
  "keywords": [
    "vue-simple-chatbot",
    "vue chatbot",
    "chatbot ui",
    "simple chatbot"
  ],
  "author": "Mochammad Fadholi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/asmfadholi/vue-simple-chatbot/issues"
  },
  "homepage": "https://github.com/asmfadholi/vue-simple-chatbot#readme"
}
